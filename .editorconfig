# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
indent_size = 2
continuation_indent_size = 4

[*.{groovy,java,kt}]
indent_style = space
indent_size = 4
continuation_indent_size = 8
